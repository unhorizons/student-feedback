{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "beberlei/doctrineextensions": "^1.3",
        "devscast/ddd-bundle": "^1.0",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.14",
        "endroid/qr-code-bundle": "^4.0",
        "erusev/parsedown": "^1.7",
        "knplabs/knp-paginator-bundle": "^5.9",
        "knpuniversity/oauth2-client-bundle": "^2.10",
        "league/glide": "^2.2.2",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.15",
        "scheb/2fa-backup-code": "^6.0",
        "scheb/2fa-bundle": "^6.0",
        "scheb/2fa-email": "^6.0",
        "scheb/2fa-google-authenticator": "^6.0",
        "scheb/2fa-trusted-device": "^6.0",
        "sensio/framework-extra-bundle": "^6.1",
        "symfony/asset": "6.2.*",
        "symfony/console": "6.2.*",
        "symfony/doctrine-messenger": "6.2.*",
        "symfony/dotenv": "6.2.*",
        "symfony/expression-language": "6.2.*",
        "symfony/flex": "^2",
        "symfony/form": "6.2.*",
        "symfony/framework-bundle": "6.2.*",
        "symfony/http-client": "6.2.*",
        "symfony/intl": "6.2.*",
        "symfony/lock": "6.2.*",
        "symfony/mailer": "6.2.*",
        "symfony/mime": "6.2.*",
        "symfony/monolog-bundle": "^3.0",
        "symfony/notifier": "6.2.*",
        "symfony/process": "6.2.*",
        "symfony/property-access": "6.2.*",
        "symfony/property-info": "6.2.*",
        "symfony/rate-limiter": "6.2.*",
        "symfony/runtime": "6.2.*",
        "symfony/security-bundle": "6.2.*",
        "symfony/serializer": "6.2.*",
        "symfony/string": "6.2.*",
        "symfony/translation": "6.2.*",
        "symfony/twig-bundle": "6.2.*",
        "symfony/uid": "6.2.*",
        "symfony/ux-autocomplete": "^2.3",
        "symfony/ux-chartjs": "^2.3",
        "symfony/ux-cropperjs": "^2.3",
        "symfony/ux-dropzone": "^2.3",
        "symfony/ux-turbo": "^2.3",
        "symfony/validator": "6.2.*",
        "symfony/web-link": "6.2.*",
        "symfony/yaml": "6.2.*",
        "twig/cssinliner-extra": "^3.3",
        "twig/extra-bundle": "^3.3",
        "twig/markdown-extra": "^3.3",
        "twig/string-extra": "^3.3",
        "twig/twig": "^2.12|^3.0",
        "vich/uploader-bundle": "^1.21"
    },
    "require-dev": {
        "symfony/maker-bundle": "^1.0",
        "symfony/browser-kit": "*",
        "symfony/css-selector": "*",
        "symfony/debug-bundle": "*",
        "symfony/panther": "^2.0",
        "symfony/phpunit-bridge": "^6.1",
        "symfony/stopwatch": "*",
        "symfony/web-profiler-bundle": "*",

        "dama/doctrine-test-bundle": "^6.7",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "hautelook/alice-bundle": "^2.10",
        "nelmio/alice": "^3.10",
        "phpstan/phpstan": "^1.4",
        "phpstan/phpstan-doctrine": "^1.2",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpstan/phpstan-symfony": "^1.1",
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^0.12.16",
        "squizlabs/php_codesniffer": "^3.6",
        "symplify/easy-coding-standard": "^10.1",
        "theofidry/alice-data-fixtures": "^1.5",
        "vimeo/psalm": "^4.21"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true,
            "endroid/installer": false,
            "composer/package-versions-deprecated": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Application\\": "src/Application/",
            "Domain\\": "src/Domain/",
            "Infrastructure\\": "src/Infrastructure/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Fixtures\\": "fixtures/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "6.2.*",
            "docker": false,
            "endpoint": [
                "https://raw.githubusercontent.com/devscast/symfony-repices/flex/main/index.json",
                "flex://defaults"
            ]
        }
    }
}
